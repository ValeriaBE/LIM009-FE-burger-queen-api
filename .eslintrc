{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "no-shadow": "off",
    "no-nested-ternary": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["e2e/**/*.js", "**/*.spec.js"]}],
    "no-console": ["off", {
      "allow": ["info", "warn", "error"]
    }],
    "no-path-concat":"off",
    "prefer-template": "off"
  }
}
